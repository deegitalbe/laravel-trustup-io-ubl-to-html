{
  "name": "deegitalbe/laravel-trustup-io-ubl-to-html",
  "description": "Basic versioned php package.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Henrotay",
      "email": "mathieu.henrotay@gmail.com"
    }
  ],
  "require": {
    "henrotaym/laravel-package-versioning-config": "^1.3",
    "php": "^8.1",
    "ext-xsl": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0",
    "pestphp/pest": "^2.30"
  },
  "autoload": {
    "psr-4": {
      "Deegitalbe\\LaravelTrustupIoUblToHtml\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deegitalbe\\LaravelTrustupIoUblToHtml\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Deegitalbe\\LaravelTrustupIoUblToHtml\\Providers\\LaravelTrustupIoUblToHtmlServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
